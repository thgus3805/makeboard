<div class= "container">
    <form action= "/update_real/<%= @one_post.id %>" method="POST">
<form>
  <div class="form-group">
    <label for="exampleInputEmail1">제목</label>
    <input value="<%= @one_post.db_title %>" name= "title" type="text" class="form-control" id="exampleInputEmail1" placeholder="제목을 입력하세요">
  </div>
  <div class="form-group">
    <label for="exampleInputPassword1">내용</label>
    <textarea name= "content" rows= "15" class="form-control" id="exampleInputPassword1" placeholder="내용을 입력하세요"><%= @one_post.db_content %></textarea>
  </div>
  <button type="submit" class="btn btn-primary">제출</button>
</form>
</div>